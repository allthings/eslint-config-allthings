{
  "name": "@allthings/eslint-config",
  "version": "3.0.0",
  "description": "ESlint shareable config for Allthings style",
  "main": "index.js",
  "files": [
    "index.js",
    "node.js"
  ],
  "scripts": {
    "lint": "prettier --check .",
    "test": "eslint test/",
    "predeploy": "yarn test",
    "deploy": "yarn publish --new-version $npm_package_version --tag latest --access public",
    "postdeploy": "git push --tags origin HEAD",
    "deps": "yarn upgrade-interactive --latest"
  },
  "dependencies": {
    "@rushstack/eslint-patch": "^1.10.3",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^17.8.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-typescript-sort-keys": "^3.2.0",
    "eslint-plugin-unicorn": "^53.0.0"
  },
  "peerDependencies": {
    "eslint": ">=8.22.0",
    "prettier": ">=3.2.4",
    "typescript": ">=4"
  },
  "devDependencies": {
    "eslint": "^9.4.0",
    "prettier": "^3.3.1",
    "typescript": ">=5.4.5"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "typescript"
  ],
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "useTabs": false
  },
  "repository": "git@github.com:allthings/eslint-config-allthings.git",
  "author": "Dmitry Markov <forall@stylive.com>",
  "license": "MIT"
}
